set nocompatible
set number
set shiftwidth=4
set softtabstop=4
set tabstop=4
set fileencodings=ucs-bom,utf-8,gbk
set guifont=Monospace\ 16
set autoindent
set smartindent
set showmatch
set ruler
set history=1000
set incsearch
set nobackup
filetype on
let Tlist_Show_One_File=1
let Tlist_Exit_OnlyWindow=1
